{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "rootDirs": [
      "./",
      "../dto/"
    ],
    "baseUrl": "./",
    "paths": {
      "~tetris/dto/*": ["../dto/*"]
    },
    "incremental": true
  },
  "exclude": []
}
