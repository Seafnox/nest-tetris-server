{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "rootDir": "../",
    "baseUrl": "./",
    "paths": {
      "~tetris/dto/*": "../dto/*",
      "~tetris/back/*": "../back/src/*",
      "~tetris/front/*": "../front/src/*"
    },
    "incremental": true
  },
  "exclude": []
}
